indent_style = space
insert_final_newline = true
ij_continuation_indent_size = 4

[*.java]
ij_java_spaces_within_array_initializer_braces = true

[{*.yaml,*.yml}]
indent_size = 4

[{*.xml}]
indent_size = 4

[{*.sbt}]
tab_width = 4
indent_size = 4
indent_style = space
