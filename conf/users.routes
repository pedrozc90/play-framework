# conf/users.routes
# Prefix: /api/users

GET           /           @web.controllers.users.UserController.fetch(page: Int ?= 1, rows: Int ?= 15, q: String ?= null, active: java.lang.Boolean ?= null)
POST        /           @web.controllers.users.UserController.save(req: Request)
GET         /:id        @web.controllers.users.UserController.get(id: Long)
PUT         /:id        @web.controllers.users.UserController.update(req: Request, id: Long)
DELETE        /:id        @web.controllers.users.UserController.delete(id: Long)
